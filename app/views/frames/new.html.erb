<div class="padded-page">
  <h1>New Frame</h1>
  <div class="padded-page__body">
    <%= form_for @frame do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>

      <div class="form-group">
        <%= f.label :email %>
        <%= f.email_field :email %>
      </div>

      <div class="form-group">
        <%= f.label :subject %>
        <%= f.text_field :subject %>
      </div>

      <div class="form-actions">
        <%= f.submit 'Save' %>
      </div>
    <% end %>
  </div>
</div>
